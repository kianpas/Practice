import java.util.Scanner;

public class Gugudan {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("1부터 9까지의 임의 수를 입력해주세요 : ");
		int num = sc.nextInt();
		sc.close();
		System.out.println();
		if (num<10) {
			for(int i=1; i<10; i++) {
				System.out.println(num+" * "+i+" = "+(num*i));
				
			}
		} else {
			System.out.println("1부터 9까지의 다시 입력해주세요");
				
		}
	}
	
}
