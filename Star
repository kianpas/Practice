import java.util.Scanner;

public class beackstar {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
				
		for(int i=0;i<n;i++)  //ì¤„
		{	
			for(int j=0;j>i-n;j--)  
			{
				System.out.print(" ");
			}
			for(int k=0;k<i+1;k++)
			{
				System.out.print("*");
			}
		System.out.println();
		}
	}
}
